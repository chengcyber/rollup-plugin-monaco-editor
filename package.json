    "@size-limit/preset-small-lib": "^4.7.0",
    "@types/es-module-lexer": "^0.3.0",
    "@types/fs-extra": "^9.0.11",
    "@types/lodash": "^4.14.170",
    "@types/semver": "^7.3.6",
    "fs-extra": "^10.0.0",
    "http-server": "^0.12.3",
    "husky": "^4.3.0",
    "monaco-editor": "0.25.0",
    "rollup": "^2.33.1",
    "rollup-plugin-postcss": "^3.1.8",
    "size-limit": "^4.7.0",
    "tempy": "^1.0.1",
    "ts-node": "^10.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "es-module-lexer": "^0.3.26",
    "recast": "^0.20.4",
    "semver": "^7.3.5"
  }
