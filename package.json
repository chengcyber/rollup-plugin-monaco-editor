    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {}
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "useTabs": false,
    "singleQuote": true,
